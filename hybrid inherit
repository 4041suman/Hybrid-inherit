class Hybrid {
    public static void main(String[] args) {
      Mother obj = new Mother();
       Uncle ob = new Uncle();
       Son nn=new Son();
        obj.sample();
        System.out.println(obj.gender);
        ob.sample();
        System.out.println(ob.gender);
        nn.sample();
        System.out.println(nn.gender);
    }
}
class GrandFather{
    char gender ='M';
    void sample(){
        System.out.println("Grandfa");
    }
}
class Mother extends GrandFather{
    char gender ='F';
    void sample(){
        System.out.println("Mother");
    }
}
class Uncle extends GrandFather{
    char gender ='M';
    void sample(){
        System.out.println("Uncle");
    }
}
class Son extends Uncle{
   char gender ='M';
    void sample(){
        System.out.println("Son");
    }
}
